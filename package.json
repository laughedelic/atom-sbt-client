{
  "name": "atom-sbt-client",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "sbt server support for Atom IDE",
  "author": {
    "name": "Alexey Alekhin",
    "email": "laughedelic@gmail.com",
    "url": "https://github.com/laughedelic"
  },
  "keywords": [
    "build",
    "scala",
    "sbt",
    "ide",
    "language-server",
    "language-server-protocol"
  ],
  "repository": "https://github.com/laughedelic/atom-sbt-client",
  "bugs": "https://github.com/laughedelic/atom-sbt-client/issues",
  "license": "MPL-2.0",
  "engines": {
    "atom": ">=1.21.0 <2.0.0"
  },
  "enhancedScopes": [
    "source.scala"
  ],
  "dependencies": {
    "atom-languageclient": "0.8.2",
    "atom-package-deps": "4.6.1"
  },
  "package-deps": [ "language-scala", "atom-ide-ui" ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "hyperclick": {
      "versions": {
        "0.1.0": "provideHyperclick"
      }
    }
  }
}
